{
  "extends": "../tslint.json",
  "rules": {
    "@angular-eslint/directive-selector": [
      true,
      {
        "type": "attribute",
        "prefix": "app",
        "style": "camelCase"
      }
    ],
    "@angular-eslint/component-selector": [
      true,
      {
        "type": "element",
        "prefix": "app",
        "style": "kebab-case"
      }
    ]
  }
}